{
  "name": "track-miner",
  "version": "1.0.0",
  "description": "fkn high as fk lol",
  "main": "./src/index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enspiral-cherubi/track-miner"
  },
  "author": "data-doge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enspiral-cherubi/track-miner/issues"
  },
  "homepage": "https://github.com/enspiral-cherubi/track-miner",
  "dependencies": {
    "jquery": "^2.2.1",
    "lodash.range": "^3.1.2",
    "raf-loop": "^1.1.3",
    "soundcloud-audio-interface": "^1.0.3",
    "three": "^0.74.0",
    "three-window-resize": "^1.0.4",
    "web-audio-analyser-2": "^2.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "envify": "^3.4.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
